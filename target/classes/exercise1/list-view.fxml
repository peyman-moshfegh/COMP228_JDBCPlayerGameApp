<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="720.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="exercise1.ListController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TableView fx:id="table" maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="gameTitle" prefWidth="200.0" text="Game Title" />
            <TableColumn fx:id="playingDate" prefWidth="200.0" text="Playing Date" />
            <TableColumn fx:id="score" prefWidth="200.0" text="Score" />
        </columns>
      </TableView>
   </children>
</VBox>
